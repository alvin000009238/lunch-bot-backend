{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "npm start",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "cron": [
      {
        "schedule": "*/5 * * * *",
        "command": "curl -s -X POST https://${RAILWAY_PUBLIC_DOMAIN}/api/cron-test"
      }
    ]
  }
}
